setenv PATH /usr/local/bin:/usr/bin:/bin:/opt/bin:/opt/include
setenv PATH ./:$PATH
setenv PATH /home/nmolem/bin:$PATH
setenv MANPATH /usr/local/man:/usr/share/man

# For python anaconda
setenv PATH /home/ddevin/anaconda2/bin:$PATH

# for NHMG
unlimit

setenv ROMS_ROOT /home/ddevin/repos-devin-maya/ucla-roms

setenv MPI_ROOT /opt/mpich-3.2
setenv MPI_INC /opt/mpich-3.2/include
setenv MPI_LIB /opt/mpich-3.2/lib

setenv NETCDF_LIB /opt/netcdf-4.6.1/lib
setenv NETCDF_INC /opt/netcdf-4.6.1/include

# CPATH needed to find .mod files for modules
setenv CPATH /opt/mpich-3.2/include
setenv CPATH ${ROMS_ROOT}:$CPATH
setenv CPATH ${NETCDF_INC}:${CPATH}

setenv NCO_ROOT /opt/nco-4.3.1
setenv PATH ${NCO_ROOT}/bin:${PATH}

setenv HDF5_LIB /opt/hdf5-1.8.20/lib
setenv HDF5_INC /opt/hdf5-1.8.20/include

# Environmental variables related to Intel
set IFC_ROOT = /home/intel/composer_xe_2013_sp1.3.174

if (`uname -m` == x86_64) then
        set IFC_ARCH_TYPE = intel64
else
        set IFC_ARCH_TYPE = ia32
endif
setenv PATH ${IFC_ROOT}/bin/${IFC_ARCH_TYPE}:${PATH}
setenv LD_LIBRARY_PATH ${IFC_ROOT}/compiler/lib/${IFC_ARCH_TYPE}
setenv MANPATH ${IFC_ROOT}/man:${MANPATH}

# limit stacksize unlimited

setenv PATH $MPI_ROOT/bin:$PATH
setenv LD_LIBRARY_PATH ${LD_LIBRARY_PATH}:$MPI_ROOT/lib

#for NHMG

setenv LD_LIBRARY_PATH ${LD_LIBRARY_PATH}:/home/nmolem/CROCO-NH/lib
setenv LD_LIBRARY_PATH ${LD_LIBRARY_PATH}:${NETCDF_LIB}
setenv LD_LIBRARY_PATH ${LD_LIBRARY_PATH}:${HDF5_LIB}

# DDol to try ncio library
setenv LD_LIBRARY_PATH ${LD_LIBRARY_PATH}:/home/ddevin/JNM_git_code/lib

alias setONE "setenv OMP_NUM_THREADS 1"
alias setTWO "setenv OMP_NUM_THREADS 2"
alias setFOUR "setenv OMP_NUM_THREADS 4"
alias setEIGHT "setenv OMP_NUM_THREADS 8"

# aliasses
#
alias psg  'ps -u nmolem | grep'
alias prg 'ls -lt /scratch/nmolem/*.000.nc|more |grep his'
alias ll  'ls -lh '
alias vw  'ncview splash_his.00??.190.nc'
alias vw2 'ncview splash_his.00??.182.nc'
alias ar 'auto_ret < /dev/null > & auto_ret.log &'

alias rc_home 'rclone sync --exclude-from /home/nmolem/rclone_exclude /home/nmolem/ gdrive:BACKUPS/Maya_home/'
alias lt 'ls -lth | head -20'
alias ltr 'ls -lhtr | tail -20'


unlimit
setONE




